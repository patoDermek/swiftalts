        <div class="navbar">
            <img class="logo" src="images/logo.png">
            <nav>
                <ul class="nav-links">
                    <li><a href="">Store</a></li>
                    <li><a href="https://twitter.com/SwiftAltsNet" target="_blank">Twitter</a></li>
                    <li><a href="https://discord.gg/wSYTd9SUCq" target="_blank">Discord</a></li>
                </ul>
            </nav>
        </div>

        <div class="section-header-background">
            <div class="section-header-background-main">
                <div class="section-header-background-title">SwiftAlts</div>
                <div class="section-header-background-description">Cheap High-Quality Minecraft Alt Shop</div>
            </div>
        </div>

        <a href="#section-main">
            <button class="goToMainSectionButton" id="goToMainSectionButton"><p>↓</p></button>
        </a>
    </div>

